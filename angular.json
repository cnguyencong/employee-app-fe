{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"employee-app-fe": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss",
					"skipTests": true
				},
				"@schematics/angular:class": {
					"skipTests": true
				},
				"@schematics/angular:directive": {
					"skipTests": true
				},
				"@schematics/angular:guard": {
					"skipTests": true
				},
				"@schematics/angular:interceptor": {
					"skipTests": true
				},
				"@schematics/angular:pipe": {
					"skipTests": true
				},
				"@schematics/angular:service": {
					"skipTests": true
				}
			},
			"root": "",
			"sourceRoot": "src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"allowedCommonJsDependencies": [
							"lodash",
							"rxjs-compat",
							"@ks89/angular-modal-gallery",
							"@ckeditor/ckeditor5-build-classic",
							"clone-deep",
							"dropzone",
							"leaflet",
							"feather-icons",
							"mousetrap",
							"hammerjs",
							"firebase/app",
							"dragula",
							"knob",
							"chartist",
							"chartjs-plugin-streaming",
							"@firebase/app",
							"chart.js",
							"dom-set",
							"crossvent",
							"contra/emitter",
							"ev-emitter/ev-emitter",
							"get-size/get-size",
							"outlayer/outlayer",
							"rxjs",
							"ngx-masonry",
							"@firebase/component",
							"@firebase/firestore",
							"@firebase/auth",
							"@firebase/util",
							"rxjs/operators",
							"rxjs/Subject",
							"rxjs/internal/operators",
							"rxjs/internal/observable/of",
							"rxjs/add/operator/map",
							"rxjs/Subscription",
							"masonry-layout",
							"imagesloaded",
							"rxjs-compat/add/operator/map",
							"@mattlewis92/dom-autoscroller",
							"dom-plane",
							"@mattlewis92/dom-autoscroller",
							"@tweenjs/tween.js",
							"photoswipe",
							"photoswipe/dist/photoswipe-ui-default",
							"angular-calendar/date-adapters/date-fns",
							"apexcharts",
							"apexcharts/dist/apexcharts.min.js",
							"calendar-utils/date-adapters/date-fns"
						],
						"outputPath": "dist/employee-app-fe",
						"index": "src/index.html",
						"main": "src/main.ts",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"src/assets/images/favicon.png",
							{
							  "glob": "**/*",
							  "input": "src/assets/",
							  "ignore": ["**/*.scss"],
							  "output": "/assets/"
							},
							"src/assets"
						],
						"styles": ["node_modules/font-awesome/scss/font-awesome.scss", "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.carousel.min.css", "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.theme.default.min.css", "src/styles.scss"],
						"scripts": [],
						"vendorChunk": true,
						"extractLicenses": false,
						"buildOptimizer": false,
						"sourceMap": true,
						"optimization": false,
						"namedChunks": true,
						"extraWebpackConfig": "webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
								  "type": "initial",
								  "maximumWarning": "9mb",
								  "maximumError": "9mb"
								},
								{
								  "type": "anyComponentStyle",
								  "maximumWarning": "9mb",
								  "maximumError": "9mb"
								}
							],
							"fileReplacements": [
								{
								  "replace": "src/environments/environment.ts",
								  "with": "src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"outputHashing": "all",
							"extraWebpackConfig": "webpack.prod.config.js"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "employee-app-fe:build:production",
							"extraWebpackConfig": "webpack.prod.config.js"
						},
						"development": {
							"browserTarget": "employee-app-fe:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4200,
						"publicHost": "http://localhost:4200",
						"extraWebpackConfig": "webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "employee-app-fe:build",
						"extraWebpackConfig": "webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "src/test.ts",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "tsconfig.spec.json",
						"karmaConfig": "karma.conf.js",
						"assets": ["src/favicon.ico", "src/assets"],
						"styles": ["src/styles.scss"],
						"scripts": []
					}
				},
				"e2e": {
					"builder": "@angular-devkit/build-angular:protractor",
					"options": {
					  "protractorConfig": "e2e/protractor.conf.js",
					  "devServerTarget": "cuba:serve"
					},
					"configurations": {
					  "production": {
						"devServerTarget": "cuba:serve:production"
					  }
					}
				}
			}
		}
	},
	"cli": {
		"analytics": "fb5769cf-2789-4927-bc80-4d49091c0164"
	},
	"schematics": {
		"@schematics/angular:component": {
		  "style": "scss"
		}
	}
}